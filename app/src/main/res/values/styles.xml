<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="gongneng_pic">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">25dp</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
    </style>

    <style name="img_left_exit">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
    </style>

    <style name="follow_info">
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/eoc_text_color_drak</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="tab_style">
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="tab_txt_selected">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/eoc_text_color_green</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="tab_txt_unselected">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/eoc_grey</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="main_ui_size">
        <item name="android:layout_width">35dp</item>
        <item name="android:layout_height">35dp</item>
    </style>

    <style name="tuijian_headpic_size">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
    </style>

    <style name="bnv_normal">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="default_margin">
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:layout_marginRight">15dp</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>

    <style name="view_widthmatch_height1dp">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/view_linear</item>
    </style>
</resources>